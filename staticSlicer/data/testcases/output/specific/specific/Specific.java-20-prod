public class WhileIf {

    public static void main(String[] args) {

        int prod = 1;
        int i = 0;
        while (i < 10) {
            i = i + 1;
            if (prod > 100 / i) {

                break;
            }


            prod = prod * i;
        }


        System.out.println(prod);
    }
}
